<form method="post" action="{{ path('app_biblio_delete_front', {'id': biblio.id}) }}" onsubmit="return confirm('Are you sure you want to delete this library?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ biblio.id) }}">
    <button class="btn btn-outline-danger btn-sm" style="margin-left:270px;margin-top:-70px;">Delete</button>
</form>
